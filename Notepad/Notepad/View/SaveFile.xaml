<Window x:Class="Notepad.View.SaveFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notepad.View"
        mc:Ignorable="d"
        Title="SaveFile" Height="180" Width="350">
    <Grid Margin="0,0,0,-16">
        <Button HorizontalAlignment="Left" Margin="45,95,0,0" VerticalAlignment="Top">
            <Button Content="Yes" Click="Button_Click"/>
        </Button>
        <Button Content="No" HorizontalAlignment="Left" Margin="175,95,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="285,95,0,0" VerticalAlignment="Top" Click="Button_Click_2"/>
        <Label Content="Save changes?" HorizontalAlignment="Left" Margin="140,29,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
