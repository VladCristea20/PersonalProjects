        xmlns:local="clr-namespace:Notepad.View"
        mc:Ignorable="d"
        Title="FindDialog" Height="240" Width="250">
    <Grid>
        <TextBox HorizontalAlignment="Left" Name="textBox" Margin="75,78,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Find" HorizontalAlignment="Left" Margin="37,74,0,0" VerticalAlignment="Top"/>
        <Button Content="Find" HorizontalAlignment="Left" Margin="108,122,0,0" VerticalAlignment="Top" Click="Button_Click"/>
